{
  "name": "music-bot",
  "version": "0.0.1",
  "description": "A music bot written using @discordjs/voice",
  "scripts": {
    "start": "yarn build && node -r tsconfig-paths/register dist/bot",
    "start:prod": "node dist/bot.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier --write **/*.{ts,js,json}",
    "build": "tsc",
    "build:check": "tsc --noEmit --incremental false"
  },
  "author": "Pedro Mol",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "0.7.0",
    "@discordjs/voice": "0.7.5",
    "ansi-regex": "6.0.1",
    "discord-api-types": "0.24.0",
    "discord-player": "^5.1.0",
    "discord.js": "13.3.1",
    "libsodium-wrappers": "0.7.9",
    "youtube-dl-exec": "2.0.0",
    "youtube-search": "^1.1.6",
    "ytdl-core": "4.9.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  }
}
